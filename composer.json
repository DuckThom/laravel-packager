{
    "name": "luna/laravel-packager",
    "description": "Create a Laravel package template with ease",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Wiringa",
            "email": "thomas.wiringa@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4",
        "illuminate/support": "^5.0",
        "illuminate/console": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Luna\\Packager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    }
}
